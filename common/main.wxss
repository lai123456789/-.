.weui-cells {
  position: relative;
  margin-top: 30rpx;
  background-color: #ffffff;
  line-height: 1.41176471;
  font-size: 28rpx;
  color: #333;
}

.weui-cells:before {
  position: absolute;
  content: "";
  top: 0;
  left: 0;
  right: 0;
  height: 1px;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #eee;
}

.weui-cells:after {
  position: absolute;
  content: "";
  bottom: 0;
  left: 0;
  right: 0;
  height: 1px;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #eee;
}

.weui-cells__title {
  margin-top: 0.77em;
  margin-bottom: 0.3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #999999;
  font-size: 28rpx;
}

.weui-cells_after-title {
  margin-top: 0;
}

.weui-cells__tips {
  margin-top: 0.3em;
  color: #999999;
  padding-left: 15px;
  padding-right: 15px;
  font-size: 28rpx;
}

.weui-cell {
	background: #fff;
	padding: 30rpx;
	position: relative;
	display: flex;
	align-items: center;
}

.weui-cell:after {
	content: "";
	position: absolute;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 30rpx;
}

.weui-cell:last-child:after {
	display: none;
}

.noafter:after {
	display: none;
}

.weui-cell_active {
	background-color: #ececec;
}

.weui-cell_primary {
	align-items: flex-start;
}

.weui-cell__bd {
	flex: 1;
}

.weui-cell__ft {
	text-align: right;
	color: #999999;
}

.weui-cell_access {
	color: inherit;
}

.weui-cell__ft_in-access {
	padding-right: 13px;
	position: relative;
}

.weui-cell__ft_in-access:after {
	content: " ";
	display: inline-block;
	height: 16rpx;
	width: 16rpx;
	border-width: 3.5rpx 3.5rpx 0 0;
	border-color: #999999;
	border-style: solid;
	-webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
	transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
	position: relative;
	position: absolute;
	top: 50%;
	margin-top: -7rpx;
	right: 2px;
}

.weui-cell_link {
	color: #586c94;
	font-size: 28rpx;
}

.weui-cell_link:active {
	background-color: #ececec;
}

.weui-cell_link:first-child:before {
