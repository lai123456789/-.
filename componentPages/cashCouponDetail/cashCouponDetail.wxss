/* 新增如下 */
.contentChange {
	width: 223rpx;
	height: 48rpx;
	/* background-color: rgba(0, 0, 0, .3); */
	/* background: red;
	opacity: 1; */
	background-color: rgba(0, 0, 0, .3);
	color: white;
	position: absolute;
	right: 0rpx;
	/* top: 250rpx; */
	bottom: 60rpx;
	border-top-left-radius: 32rpx;
	border-bottom-left-radius: 32rpx;
}

.contentChange .neirong {
	display: flex;
	align-items: center;
	height: 100%;
	justify-content: center;
	/* css 2分之一 2s  js两倍* 4s */
	animation: blurFadeIn 5s infinite 0s backwards;
	/*  infinite ease-in*/
	color: transparent;
	text-shadow: 0px 0px 0rpx #fff;
	animation-direction: alternate;
}

@keyframes blurFadeIn {
	0% {
		opacity: 0;
		/* text-shadow: 0px 0px 0rpx #fff;/*  10rpx*/
		transform: scale(1.2);
		/* transform: scale(1); */
	}

	50% {
		opacity: 1;
		/* text-shadow: 0px 0px 0rpx #fff; /*  6rpx*/
		transform: scale(1);
	}

	75% {
		opacity: 1;
		/* text-shadow: 0px 0px 0rpx #fff; */
		transform: scale(1);
	}

	100% {
		opacity: 1;
		/* text-shadow: 0px 0px 0rpx #fff; */
		transform: scale(1);
	}
}
