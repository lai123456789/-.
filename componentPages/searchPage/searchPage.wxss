.search_box {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
}

.searchbox_input {
  width: 588rpx;
  margin-left: 30rpx;
  position: relative;
  /* margin-top: 14rpx; */
  background-color: #f5f5f5;
  border-radius: 30rpx;
  /* width: 305rpx; */
}

.searchbox_input input {
  padding-left: 63rpx;
  background-color: #f5f5f5;
  height: 60rpx;
  border-radius: 30rpx;
}

.fdj {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 15rpx;
  left: 20rpx;
}
.clear {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 24rpx;
  top: 14rpx;
  z-index: 2;
}
.history_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.history_list {
  display: flex;
  flex-wrap: wrap;
}
.history_item {
  padding: 11rpx 34rpx;
  background-color: #f4f4f4;
  margin-top: 20rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
}
/* 筛选框吸顶样式 */
.search_card {
  height: 88rpx;
}
.search_card.fixed {
  background-color: #fff;
  position: fixed;
  z-index: 3;
  width: 100%;
}
